{
  "manifest_version": 3,
  "name": "Galaxy Wallet",
  "version": "1.0",
  "description": "A Chrome extension crypto wallet for Bittensor",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Galaxy Wallet"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  }
}
